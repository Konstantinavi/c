#include <iostream>
#include <cmath> 
using namespace std;
double F(double x) {
    return x * x * x - 4.2 / (x * x - 2.25) - pow(cos(x), 2);
}
int main() {
    double xn, xk, dx;
    cout << "Введите начальное значение x : ";
    cin >> xn;
    cout << "Введите конечное значение x : ";
    cin >> xk;
    cout << "Введите шаг : ";
    cin >> dx;
    cout << "\nТаблица значений функции F(x):\n";
    cout << "--------------------------\n";
    cout << "x |F(x)\n";
    cout << "--------------------------\n";
    double x = xn;
    while (x <= xk) {
        double y = F(x);
        cout  <<x << " | " <<y << endl;
        x += dx;
    }
    cout << "--------------------------\n";

    return 0;
}
